<<<<<<< HEAD
#include <iostream>
using namespace std;

int main() {
    int rows = 5;

    for (int i = rows; i >= 1; i--) {
        
        for (int s = 1; s < i; s++) {
            cout << " ";
        }

        
        for (char ch = 'A' + i - 1; ch <= 'E'; ch++) {
            cout << ch << " ";
        }

        cout << endl;
    }

    return 0;
}
=======
#include <iostream>
using namespace std;

int main() {
    int rows = 5;

    for (int i = rows; i >= 1; i--) {
        
        for (int s = 1; s < i; s++) {
            cout << " ";
        }

        
        for (char ch = 'A' + i - 1; ch <= 'E'; ch++) {
            cout << ch << " ";
        }

        cout << endl;
    }

    return 0;
}
